<?xml version='1.0' encoding='utf-8'?>
<metamodel xmlns="http://schema.real.com/schema/" >
	<include>kernel_metamodel.xml</include>
	<namespace>UML 2.1</namespace>
	<diagram name="Class Diagram" >
		<graphicTypes>
			<node name="Class" >
				<picture sizex="150" sizey="210" >
					<rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" x1="0" y2="210" stroke-width="1" x2="150" fill-style="solid" />
					<line fill="#000000" stroke-style="solid" stroke="#000000" y1="25a" x1="0" y2="25a" stroke-width="1" x2="150" fill-style="solid" />
					<line fill="#000000" stroke-style="solid" stroke="#000000" y1="100" x1="0" y2="100" stroke-width="1" x2="150" fill-style="solid" />
				</picture>
				<labels>
					<label x="40" y="10" textBinded="name" />
				</labels>
				<ports>
					<linePort>
						<start startx="0" starty="0" />
						<end endx="150" endy="0" />
					</linePort>
					<linePort>
						<start startx="150" starty="0" />
						<end endx="150" endy="210" />
					</linePort>
					<linePort>
						<start startx="150" starty="210" />
						<end endx="0" endy="210" />
					</linePort>
					<linePort>
						<start startx="0" starty="210" />
						<end endx="0" endy="0" />
					</linePort>
				</ports>
				<logic>
					<generalizations>
						<parent parentName="Classifier" />
					</generalizations>
					<properties>
						<property type="string" name="vals" />
						<property type="string" name="methods" />
						<property type="string" name="logicalName" />     
					</properties>
				</logic>
			</node>
			<node name="View" >
				<picture sizex="100" sizey="100" >
					<text i="0" font-fill="#0000ff" y1="7a" b="0" x1="40%" font-name="MS Shell Dlg 2" u="0" font-size="15a" >View</text>
					<line fill="#000000" stroke-style="solid" stroke="#008000" y1="20a" x1="0a" y2="20a" stroke-width="2" x2="100%" fill-style="solid" />
					<line fill="#000000" stroke-style="solid" stroke="#008000" y1="100%" x1="0a" y2="100%" stroke-width="2" x2="100%" fill-style="solid" />
					<line fill="#000000" stroke-style="solid" stroke="#008000" y1="20a" x1="0a" y2="100%" stroke-width="2" x2="0a" fill-style="solid" />
					<line fill="#000000" stroke-style="solid" stroke="#008000" y1="20a" x1="100%" y2="100%" stroke-width="2" x2="100%" fill-style="solid" />
				</picture>
				<labels>
					<label x="40" y="10" textBinded="name" />
				</labels>
				<ports>
					<linePort>
						<start startx="0a" starty="20a" />
						<end endx="100%" endy="20a" />
					</linePort>
					<linePort>
						<start startx="0a" starty="100%" />
						<end endx="100%" endy="100%" />
					</linePort>
					<linePort>
						<start startx="0a" starty="20a" />
						<end endx="0a" endy="100%" />
					</linePort>
					<linePort>
						<start startx="100%" starty="20a" />
						<end endx="100%" endy="100%" />
					</linePort>
				</ports>
				<logic>
					<generalizations>
						<parent parentName="Classifier" />
					</generalizations>
					<properties>
						<property type="string" name="logicalName" />
					</properties>
				</logic>
			</node>
			<edge name="Aggregation" >
				<lineType type="solidLine" />
				<logic>
					<associations beginType="empty_rhomb" endType="no_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>
			<edge name="Relation" >
				<lineType type="solidLine" />
				<logic>
					<associations beginType="no_arrow" endType="no_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>
			<edge name="Composition" >
				<lineType type="solidLine" />
				<logic>
					<associations beginType="filled_rhomb" endType="no_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>
			<edge name="Dependency" >
				<lineType type="dashLine" />
				<logic>
					<generalizations>
						<parent parentName="Kernel Relationship" />
					</generalizations>
					<associations beginType="no_arrow" endType="open_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>
			<edge name="Realization" >
				<lineType type="dashLine" />
				<logic>
					<associations beginType="no_arrow" endType="empty_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>
			<edge name="Kernel Generalization" >
				<logic>
					<properties>
						<property type="bool" name="isSubstitutable" />
					</properties>
					<associations beginType="no_arrow" endType="empty_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>
			<node name="Class method" >
				<picture sizex="100" sizey="20" />
				<labels>
					<label x="40" y="10" textBinded="name" />
				</labels>
				<logic>
					<generalizations>
						<parent parentName="Classifier" />
					</generalizations>
					<properties>
						<property type="string" name="type" />
						<property type="string" name="operationFactors" />
						<property type="string" name="isStatic" />
						<property type="string" name="isSynchronized" />
						<property type="string" name="isNative" />
					</properties>
				</logic>
			</node>
			<node name="Class field" >
				<picture sizex="100" sizey="20" />
				<labels>
					<label x="40" y="10" textBinded="name" />
				</labels>
				<logic>
					<generalizations>
						<parent parentName="Classifier" />
					</generalizations>
					<properties>
						<property type="string" name="type" />
						<property type="string" name="defaultValue" />
						<property type="string" name="isStatic" />
						<property type="string" name="isTransient" />
						<property type="string" name="isVolatile" />
						<property type="string" name="isLeaf" />			
					</properties>					
				</logic>
			</node>
			<node name="Interface" >
				<picture sizex="150" sizey="210" >
					<rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" x1="0" y2="210" stroke-width="1" x2="150" fill-style="solid" />
					<line fill="#000000" stroke-style="solid" stroke="#000000" y1="50a" x1="0" y2="50a" stroke-width="1" x2="150" fill-style="solid" />
					<line fill="#000000" stroke-style="solid" stroke="#000000" y1="100" x1="0" y2="100" stroke-width="1" x2="150" fill-style="solid" />
					<text i="0" font-fill="#000000" y1="16a" b="0" x1="6a" font-name="MS Shell Dlg 2" u="0" font-size="10a" >&lt;&lt;interface>></text>
				</picture>
				<labels>
					<label x="40" y="10" textBinded="name" />
				</labels>
				<ports>
					<linePort>
						<start startx="0" starty="0" />
						<end endx="150" endy="0" />
					</linePort>
					<linePort>
						<start startx="150" starty="0" />
						<end endx="150" endy="210" />
					</linePort>
					<linePort>
						<start startx="150" starty="210" />
						<end endx="0" endy="210" />
					</linePort>
					<linePort>
						<start startx="0" starty="210" />
						<end endx="0" endy="0" />
					</linePort>
				</ports>
				<logic>
					<properties>
						<property type="string" name="vals" />
						<property type="string" name="methods" />
						<property type="string" name="logicalName" />     
						<property type="string" name="isAbstract" />   
					</properties>
				</logic>
			</node>
		</graphicTypes>
	</diagram>
</metamodel>
