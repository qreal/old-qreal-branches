				</logic>
			</edge>
			<edge id="krneGeneralization" name="Generalization" >
				<logic>
					<generalizations>
						<generalization>
							<parent parent_id="krneDirRelationship" />
						</generalization>
					</generalizations>
					<properties>
						<property type="bool" name="isSubstitutable" />
					</properties>
     <associations beginType="no_arrow" endType="empty_arrow">
      <association beginName="Element" endName="Element" />
     </associations>
				</logic>
			</edge>
