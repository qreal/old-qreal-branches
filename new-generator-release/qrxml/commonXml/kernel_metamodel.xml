<?xml version='1.0' encoding='utf-8'?>
<metamodel xmlns="http://schema.real.com/schema/" >
	<namespace>UML 2.1</namespace>
	<diagram name="Kernel" >
		<nonGraphicTypes>
			<enum name="State" >
				<value>Created</value>
				<value>Development</value>
				<value>Ready for testing</value>
				<value>Testing</value>
				<value>Feedback</value>
				<value>Debugging</value>
				<value>Ready for acceptance</value>
				<value>Accepted</value>
				<value>Release</value>
				<value>Launched</value>
			</enum>
			<enum name="Visibility Kind" >
				<value>private</value>
				<value>protected</value>
				<value>public</value>
				<value>package</value>
			</enum>
			<enum name="Parameter Direction Kind" >
				<value>in</value>
				<value>inout</value>
				<value>out</value>
				<value>return</value>
			</enum>
		</nonGraphicTypes>
		<graphicTypes>
			<node name="Kernel Element" >
				<logic>
					<properties>
						<property type="enum" name="state" >
							<enum nameReference="State" />
							<default>Created</default>
						</property>
					</properties>
				</logic>
			</node>
			<node name="Kernel Diagram" >
				<picture sizex="30" sizey="30">
					<path fill="#f8f8f8" stroke-style="solid" int="17" stroke="#000000" stroke-width="1" d=" M 0.9 0 C 0.402944 0 0 0.447715 0 1 L 0 19 C 0 19.5523 0.402944 20 0.9 20 L 29.1 20 C 29.5971 20 30 19.5523 30 19 L 30 1 C 30 0.447715 29.5971 0 29.1 0 L 0.9 0" fill-style="solid" />
				</picture>
				<labels>
					<label x="40" y="10" textBinded="name" />
				</labels>
				<ports>
					<pointPort x="0" y="10" />
					<pointPort x="15" y="0" />
					<pointPort x="30" y="10" />
					<pointPort x="15" y="20" />
				</ports>
				<logic>
					<properties>
						<property type="string" name="language" />
						<property type="bool" name="isView" />
					</properties>
				</logic>
			</node>			
			<node name="Named Element" >
				<logic>
					<generalizations>
						<parent parentName="Kernel Element" />
					</generalizations>
					<properties>
						<property type="string" name="name" />
						<property type="enum" name="visibility" >
							<enum nameReference="Visibility Kind" />
						</property>
						<property type="string" name="qualifiedName" />
					</properties>
				</logic>
			</node>
			<edge name="Kernel Relationship" >
				<logic>
					<properties>
						<!-- TO DISCUSS!!! -->
						<property type="string" name="from" />
						<property type="string" name="to" />
						<property type="string" name="fromPort" />
						<property type="string" name="toPort" />
						<!-- END TO DISCUSS!!! -->
						<property type="string" name="fromMultiplicity" />
						<property type="string" name="toMultiplicity" />
					</properties>
					<associations beginType="no_arrow" endType="no_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>
			<edge name="Kernel Directed Relationship" >
				<logic>
					<properties>
						<property type="string" name="condition" />
					</properties>
					<associations beginType="no_arrow" endType="open_arrow">
						<association beginName="Element" endName="Element" />
					</associations>
				</logic>
			</edge>
			<node name="Comment" >
				<logic>
					<properties>
						<property type="string" name="body" />
					</properties>
					<generalizations>
						<parent parentName="Kernel Element" />
					</generalizations>
				</logic>
			</node>
			<edge name="Comment Link" >
				<logic>
					<associations beginType="no_arrow" endType="no_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>
			<edge name="Element Import" >
				<logic>
					<generalizations>
						<parent parentName="krneDirRelationship" />
					</generalizations>		
					<properties>
						<property type="string" name="alias" />
					</properties>
					<associations beginType="no_arrow" endType="no_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>
			<edge name="Package Import" >
				<logic>
					<generalizations>
						<parent parentName="Kernel Directed Relationship" />
					</generalizations>
					<associations beginType="no_arrow" endType="no_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>		
			<node name="Redefinable Element" >
				<logic>
					<properties>
						<property type="bool" name="isLeaf" />
					</properties>
				</logic>
			</node>
			<node name="Classifier" >
				<logic>
					<generalizations>
						<parent parentName="Redefinable Element" />
					</generalizations>
					<properties>
						<property type="bool" name="isAbstract" >
							<default>false</default>
						</property>
					</properties>
				</logic>
			</node>
			<edge name="Kernel Generalization" >
				<logic>
					<generalizations>
						<parent parentName="Kernel Directed Relationship" />
					</generalizations>
					<properties>
						<property type="bool" name="isSubstitutable" />
					</properties>
					<associations beginType="no_arrow" endType="empty_arrow">
						<association beginName="Kernel Element" endName="Kernel Element" />
					</associations>
				</logic>
			</edge>
			<node name="Feature" >
				<logic>
					<generalizations>
						<parent parentName="Redefinable Element" />
					</generalizations>
					<properties>
						<property type="bool" name="isStatic" />
					</properties>
				</logic>
			</node>
			<node name="Behavioral Feature" >
				<logic>
					<generalizations>
						<parent parentFeature="Feature" />
					</generalizations>
					<properties>
						<property type="bool" name="isAbstract" />
					</properties>
				</logic>
			</node>
			<node name="Structural Feature" >
				<logic>
					<generalizations>
						<parent parentFeature="Feature" />
					</generalizations>
					<properties>
						<property type="bool" name="isReadOnly" />
					</properties>
				</logic>
			</node>
			<node name="Operation" >
				<logic>
					<generalizations>
						<parent parentName="Behavioral Feature" />
					</generalizations>
					<properties>
						<property type="bool" name="isQuery" >
							<default>false</default>
						</property>
						<property type="bool" name="isOrdered" />
						<property type="bool" name="isUnique" />
						<property type="int" name="lower" />
						<property type="positiveInt" name="upper" />
					</properties>
				</logic>
			</node>
			<node name="Parameter" >
				<logic>
					<properties>
						<property type="enum" name="direction" >
							<enum nameReference="Parameter Direction Kind" />
						</property>
					</properties>
				</logic>
			</node>
		</graphicTypes>
	</diagram>
</metamodel>
