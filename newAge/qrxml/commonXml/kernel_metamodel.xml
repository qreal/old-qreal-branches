<metamodel xmlns="http://schema.real.com/schema/">
    <namespace>UML 2.1</namespace>
    <diagram name="Kernel" nodeName="Diagram">
      <nonGraphicTypes>
        <enum name="bool">
          <value>false</value>
          <value>true</value>
        </enum>
        <enum name="Aggregation Kind">
          <value>none</value>
          <value>shared</value>
          <value>composite</value>
        </enum>
        <enum name="Visibility Kind">
          <value>private</value>
          <value>protected</value>
          <value>public</value>
          <value>package</value>
        </enum>
        <enum name="Parameter Direction Kind">
          <value>in</value>
          <value>inout</value>
          <value>out</value>
