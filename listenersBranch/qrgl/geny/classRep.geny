Task 'JavaClass'
/* Produce Java class from repo */

'class ' @@name@@ ' {'
foreach Method in children of MethodContainer
	withTab /*This section will print eith tabulation*/
	@@MethodVisibility@@ ' ' @@MethodReturnType@@ ' ' @@MethodName@@ '(' task "MethodParameters" ') { '
	'}'

foreach Field in children of FieldContainer
	withTab
	@@FieldVisibility@@ ' ' @@FieldType@@ ' ' @@FieldName@@ ';'
'}'
